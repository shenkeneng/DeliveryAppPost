apply plugin: 'com.android.application'

android {
    signingConfigs {
        release {
            keyAlias 'frxsapp'
            keyPassword 'frxs1234'
            storeFile file('D:/androidstudiospace/Order/DeliveryAppPost/frxs_key')
            storePassword 'frxs1234'
