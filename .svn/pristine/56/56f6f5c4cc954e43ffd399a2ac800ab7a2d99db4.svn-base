package com.frxs.delivery.model;

import java.io.Serializable;
import java.util.List;

/**
 * 对账单&对账详情 by Tiepier
 */
public class BillData implements Serializable {

    private static final long serialVersionUID = 4353855196071998150L;

    /**
     * 对账单&对账详情共用
     */
    private double TotalProductAmt;//总金额
    private double TotalPoint;//总积分
    private int TotalOrderCount;//总订单数
    private List<BillList> SaleOrderData;
    private double TotalAdjPoint;//总调整分
    private int ShopCount;//门店数
    private double TotalBasePointExt;//总附加分
    private double TotalBasePoint;//总绩效分
    private double TotalPoint2;// 合计（绩效分+附加分+调整分）
    /**
     * 对账详情
     */
    private String PackingTime;//时间
    private List<Integer> GroupNnumbers;


    public double getTotalProductAmt() {
        return TotalProductAmt;
    }

    public void setTotalProductAmt(double totalProductAmt) {
        TotalProductAmt = totalProductAmt;
    }

    public double getTotalPoint() {
        return TotalPoint;
    }

    public void setTotalPoint(double totalPoint) {
        TotalPoint = totalPoint;
    }

    public int getTotalOrderCount() {
        return TotalOrderCount;
    }

    public void setTotalOrderCount(int totalOrderCount) {
        TotalOrderCount = totalOrderCount;
    }

    public List<BillList> getSaleOrderData() {
        return SaleOrderData;
    }

    public void setSaleOrderData(List<BillList> saleOrderData) {
        SaleOrderData = saleOrderData;
    }

    public String getPackingTime() {
        return PackingTime;
    }

    public void setPackingTime(String packingTime) {
        PackingTime = packingTime;
    }

    public double getTotalAdjPoint() {
        return TotalAdjPoint;
    }

    public void setTotalAdjPoint(double totalAdjPoint) {
        TotalAdjPoint = totalAdjPoint;
    }

    public int getShopCount() {
        return ShopCount;
    }

    public void setShopCount(int shopCount) {
        ShopCount = shopCount;
    }

    public double getTotalBasePointExt() {
        return TotalBasePointExt;
    }

    public void setTotalBasePointExt(double totalBasePointExt) {
        TotalBasePointExt = totalBasePointExt;
    }

    public double getTotalBasePoint() {
        return TotalBasePoint;
    }

    public void setTotalBasePoint(double totalBasePoint) {
        TotalBasePoint = totalBasePoint;
    }

    public double getTotalPoint2() {
        return TotalPoint2;
    }

    public void setTotalPoint2(double totalPoint2) {
        TotalPoint2 = totalPoint2;
    }

    public List<Integer> getGroupNnumbers() {
        return GroupNnumbers;
    }

    public void setGroupNnumbers(List<Integer> GroupNnumbers) {
        this.GroupNnumbers = GroupNnumbers;
    }
}
